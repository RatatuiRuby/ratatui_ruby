class Timestamp < Data
  attr_reader milliseconds: Integer

  def self.now: () -> instance
  def self.current: () -> Integer
  def elapsed?: (Integer duration_ms) -> bool
  def self.new: (?milliseconds: Numeric) -> instance
end
